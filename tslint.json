{
    "extends": "tslint:recommended",
    "rules": {
        "await-promise": [
            true,
            "PromiseLike"
        ],
        "ban-comma-operator": true,
        "interface-name": [
            false
        ],
        "linebreak-style": [
            true,
            "LF"
        ],
        "no-any": true,
        "no-floating-promises": true,
        "object-literal-sort-keys": false,
        "prefer-for-of": false,
        "variable-name": [
            true,
            "allow-leading-underscore"
        ]
    }
}