{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "declaration": true,
        "sourceMap": false,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "."
    },
    "include": [
        "ts/index.ts",
        "ts/test/index.ts",
        "ts/typings/**/*"
    ],
    "exclude": []
}